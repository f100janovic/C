#include <stdio.h>
#include <stdlib.h>

int main()
{
	/*
		++m - odma je vrednost za 1 veca
		m++, vrednost je i dalje 1 kad se izvrsi
		za m=0
		printf("%d", m++) ispisuje 0
		printf("%d", ++m) ispisuje 1
	*/
	m=0;
	m+=m++;
	printf("%d", m);

	/*
		Ternary operator, ima manji prioritet od ||
		a ? b : c -> ako je a true onda b, inace c
		if ( (2<3) || (5<4) ? false : true){
			printf("yes"); -> ovo se ispisuje 
		}

	*/

	return 0;
}


#include <stdio.h>
#include <stdlib.h>
//najgore resenje ikad :D
int main()
{
	char tabla[9];
	int i;
	for(i=0;i<9;i++){
		scanf("%c", &tabla[i]);
	}
	for(i=1;i<10;i++){
        if(i%3==0){
            printf("%c \n", tabla[i-1]);
        }
        else{
            printf("%c", tabla[i-1]);
        }
	}
	i=0;
	if(tabla[i]=='x'&&tabla[i+1]=='x'&&tabla[i+2]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+1]=='o'&&tabla[i+2]=='o'){
        printf("o");
	}

	i=3;
	if(tabla[i]=='x'&&tabla[i+1]=='x'&&tabla[i+2]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+1]=='o'&&tabla[i+2]=='o'){
        printf("o");
	}

	i=6;
	if(tabla[i]=='x'&&tabla[i+1]=='x'&&tabla[i+2]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+1]=='o'&&tabla[i+2]=='o'){
        printf("o");
	}

	i=1;
	if(tabla[i]=='x'&&tabla[i+3]=='x'&&tabla[i+6]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+3]=='o'&&tabla[i+6]=='o'){
        printf("o");
	}

	i=2;
	if(tabla[i]=='x'&&tabla[i+3]=='x'&&tabla[i+6]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+3]=='o'&&tabla[i+6]=='o'){
        printf("o");
	}

	i=3;
	if(tabla[i]=='x'&&tabla[i+3]=='x'&&tabla[i+6]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+3]=='o'&&tabla[i+6]=='o'){
        printf("o");
	}

	i=0;
	if(tabla[i]=='x'&&tabla[i+4]=='x'&&tabla[i+8]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+4]=='o'&&tabla[i+8]=='o'){
        printf("o");
	}

	i=2;
	if(tabla[i]=='x'&&tabla[i+2]=='x'&&tabla[i+4]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+2]=='o'&&tabla[i+4]=='o'){
        printf("o");
	}



	return 0;
}



#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

//gleda da li ce k1 i k2 ikad da budu u istoj poziciji u isto vreme
//10000 pokusaja da ne bi bio infinite loop
int main()
{
	int k1,k2,s1,s2,i;
	scanf("%d %d %d %d",&k1,&s1,&k2,&s2);
	i=1;
	while(i<=10001){
        fflush(stdout);
		if(k1==k2){
			printf("YES\n");
            break;
		}
        else if(i==10000){
            printf("NO\n");
        }
		k1=k1+s1;
		k2=k2+s2;
		++i;

	}

	return 0;
}

