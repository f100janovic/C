#include <stdio.h>
#include <stdlib.h>
int main()
{
	/*
	izracunaj x iz formule ax+b=0
	*/
	float a,b;
	float x;
	scanf("%f %f",&a,&b);
	printf("%f\n",-b/a);
	return 0;
}



#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main()
{
	/*
	zvezdice
	n=4
	****
	*  *
	*  *
	****
ne radi trenutno
	*/

	int i,n,d;
	char c;
	c = '*';
	scanf("%d", &d);
	n=pow(d,2);
	for (i=1; i<=n;i++){
        if (i!=n || i!=1){
            for (i=1; i<=n;i++){
                if (i%d==0){
                    printf("%c\n", c);
                }
                else{
                    printf("%c", c);
                }
            }
        }

    }
return 0;
}



#include <stdio.h>
#include <stdlib.h>
#include <math.h>
/*
iscrtava trougao sa n redova
*/
int main()
{
    int a,i,n,b,d,x;
    a=1;
    char c = '*';
    scanf("%d", &n);
    for (x=0;x<n;x++){
        for(d=1;d<=n-x;d++){
            printf(" ");
        }
        for (b=1;b<=a;b++)
        {
            if(b%2==1){
                printf("*");
            }
            if(b%2==0){
                printf(" ");
            }

        }
        a+=2;
        printf("\n");
    }
    return 0;
}

