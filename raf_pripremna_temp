#include <stdio.h>
#include <stdlib.h>
int main()
{
	/*
	izracunaj x iz formule ax+b=0
	*/
	float a,b;
	float x;
	scanf("%f %f",&a,&b);
	printf("%f\n",-b/a);
	return 0;
}



#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main()
{
	/*
	zvezdice
	n=4
	****
	*  *
	*  *
	****
	*/

	int i,n,d,a,x;
	char c;
	c = '*';
	scanf("%d", &d);
	n=pow(d,2);
	for (i=1; i<=d;i++){
            if ((i==1)||(i==d)){
                printf("\n");
                for (a=1;a<=d;a++){
                    printf("*");
                }
            }
            else{
                printf("\n");
                for(x=1;x<=d;x++){
                    if((x==1)||(x==d)){
                        printf("*");
                    }
                    else{
                        printf(" ");
                    }
                }
            }

    }
return 0;
}




#include <stdio.h>
#include <stdlib.h>
#include <math.h>
/*
iscrtava trougao sa n redova
*/
int main()
{
    int a,i,n,b,d,x;
    a=1;
    char c = '*';
    scanf("%d", &n);
    for (x=0;x<n;x++){
        for(d=1;d<=n-x;d++){
            printf(" ");
        }
        for (b=1;b<=a;b++)
        {
            if(b%2==1){
                printf("*");
            }
            if(b%2==0){
                printf(" ");
            }

        }
        a+=2;
        printf("\n");
    }
    return 0;
}



#include <stdio.h>
#include <stdlib.h>
#include <math.h>
/*
switch case
*/
int main()
{
int i,a;
scanf("%d", &a);
switch(a){
case 2:
    printf("ok");
    break;
case 5:
    printf("not ok");
    break;
}
a=4;
}
