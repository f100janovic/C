#include <stdio.h>
#include <stdlib.h>

int main()
{
	/*
		++m - odma je vrednost za 1 veca
		m++, vrednost je i dalje 1 kad se izvrsi
		za m=0
		printf("%d", m++) ispisuje 0
		printf("%d", ++m) ispisuje 1
	*/
	m=0;
	m+=m++;
	printf("%d", m);

	/*
		Ternary operator, ima manji prioritet od ||
		a ? b : c -> ako je a true onda b, inace c
		if ( (2<3) || (5<4) ? false : true){
			printf("yes"); -> ovo se ispisuje 
		}

	*/

	return 0;
}


#include <stdio.h>
#include <stdlib.h>
//najgore resenje ikad :D
int main()
{
	char tabla[9];
	int i;
	for(i=0;i<9;i++){
		scanf("%c", &tabla[i]);
	}
	for(i=1;i<10;i++){
        if(i%3==0){
            printf("%c \n", tabla[i-1]);
        }
        else{
            printf("%c", tabla[i-1]);
        }
	}
	i=0;
	if(tabla[i]=='x'&&tabla[i+1]=='x'&&tabla[i+2]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+1]=='o'&&tabla[i+2]=='o'){
        printf("o");
	}

	i=3;
	if(tabla[i]=='x'&&tabla[i+1]=='x'&&tabla[i+2]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+1]=='o'&&tabla[i+2]=='o'){
        printf("o");
	}

	i=6;
	if(tabla[i]=='x'&&tabla[i+1]=='x'&&tabla[i+2]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+1]=='o'&&tabla[i+2]=='o'){
        printf("o");
	}

	i=1;
	if(tabla[i]=='x'&&tabla[i+3]=='x'&&tabla[i+6]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+3]=='o'&&tabla[i+6]=='o'){
        printf("o");
	}

	i=2;
	if(tabla[i]=='x'&&tabla[i+3]=='x'&&tabla[i+6]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+3]=='o'&&tabla[i+6]=='o'){
        printf("o");
	}

	i=3;
	if(tabla[i]=='x'&&tabla[i+3]=='x'&&tabla[i+6]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+3]=='o'&&tabla[i+6]=='o'){
        printf("o");
	}

	i=0;
	if(tabla[i]=='x'&&tabla[i+4]=='x'&&tabla[i+8]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+4]=='o'&&tabla[i+8]=='o'){
        printf("o");
	}

	i=2;
	if(tabla[i]=='x'&&tabla[i+2]=='x'&&tabla[i+4]=='x'){
        printf("x");
	}
	if(tabla[i]=='o'&&tabla[i+2]=='o'&&tabla[i+4]=='o'){
        printf("o");
	}



	return 0;
}
