#include <stdio.h>
#include <stdlib.h>
//n-ti fibonacijev broj
int main()
{
	int i,n,x,y,z;
	scanf("%d", &n);
	x=0;
	y=1;
	for(i=0;i<=n
	;i++){
		z=y;
		y=x+y;
		x=y;
	}
	printf("%d\n", y);
	return 0;
}



#include <stdio.h>
#include <stdlib.h>
//NP koji nalazi najveci broj u tekstu asdbadh123124bkasf23324abds -> 123124
int main()
{
    char c;
    int broj =0, max =0;
    while((c=getchar())!='\n'){
        if('0'<=c&&c<='9'){
            broj=broj*10+(c-'0');
            if(broj>max)max=broj;
        }
        else{
            broj=0;
        }
    }
    printf("%d\n", max);
	return 0;
}



#include <stdio.h>
#include <stdlib.h>
//nzs i nzd
int nzd(int x, int y){
    while(x!=y){
        if(x>y){
            x-=y;
        }
        else{
            y-=x;
        }
    }
    return x;
}

int nzs(int c,int d){
    return (c*d)/nzd(c,d);
}
int main()
{
    //nzs
    int a,b,n,k;
    scanf("%d%d", &a,&b);
    k=nzd(a,b);
    n=nzs(a,b);
    printf("%d %d\n", k, n);
	return 0;
}
